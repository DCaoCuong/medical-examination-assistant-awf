{
  "meta": {
    "schema_version": "1.1.0",
    "awf_version": "4.0.2"
  },
  "project": {
    "name": "Senlyzer - AI Medical Examination Assistant",
    "type": "Next.js Medical Platform",
    "status": "MVP - Pre-deployment"
  },
  "tech_stack": {
    "frontend": "Next.js 15, Tailwind CSS, Framer Motion",
    "api_runtime": "Next.js API Routes (Serverless)",
    "ai_services": {
      "stt": "Groq Whisper-large-v3",
      "llm": "Groq Llama-3.3-70b-versatile",
      "embeddings": "Google Vertex AI / Generative Language API"
    },
    "database": "PostgreSQL (pg pool)"
  },
  "api_endpoints": [
    {
      "path": "/api/patients",
      "method": "GET",
      "desc": "Lấy danh sách bệnh nhân"
    },
    {
      "path": "/api/patients/[id]",
      "method": "GET",
      "desc": "Thông tin chi tiết bệnh nhân"
    },
    {
      "path": "/api/patients/[id]/history",
      "method": "GET",
      "desc": "Lịch sử bệnh án của bệnh nhân"
    },
    {
      "path": "/api/bookings",
      "method": "POST",
      "desc": "Tạo phiên khám mới"
    },
    {
      "path": "/api/medical-records",
      "method": "POST",
      "desc": "Lưu kết quả SOAP và so sánh AI"
    },
    {
      "path": "/api/ai/stt",
      "method": "POST",
      "desc": "Chuyển giọng nói thành văn bản"
    },
    {
      "path": "/api/ai/analyze",
      "method": "POST",
      "desc": "Phân tích SOAP Multi-agent (Scribe, ICD, Protocol agents)"
    },
    {
      "path": "/api/ai/embed",
      "method": "POST",
      "desc": "Tạo vector embedding cho so sánh"
    },
    {
      "path": "/api/dashboard/stats",
      "method": "GET",
      "desc": "Thống kê dashboard"
    }
  ],
  "patterns": [
    "Multi-agent AI pipeline for specialized medical tasks",
    "RAG (Retrieval-Augmented Generation) for medical protocols",
    "Cosine similarity comparison for AI vs Human diagnosis",
    "Root-level dashboard navigation for medical workflow efficiency"
  ]
}